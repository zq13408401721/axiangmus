<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".presenter.acivity.Setting_acivity">
    <android.support.v7.widget.Toolbar
        android:id="@+id/tl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/fan"
            android:layout_width="@dimen/dp_9"
            android:layout_height="@dimen/dp_18"
            android:src="@mipmap/fanhuhei"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_gravity="left"
            android:layout_marginTop="@dimen/dp_14"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="设置"
            android:layout_gravity="center"
            android:textSize="17sp"
            />
    </android.support.v7.widget.Toolbar>
    <TextView
        android:id="@+id/tv_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="姓名"
        android:textSize="16sp"
        android:layout_marginTop="@dimen/dp_16"
        android:layout_marginLeft="@dimen/dp_12"
        android:layout_below="@id/tl"/>
    <TextView
        android:id="@+id/tv_names"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="dasdasdaasdas"
        android:textColor="@color/grey"
        android:layout_marginLeft="@dimen/dp_204"
        android:layout_toRightOf="@id/tv_name"
        android:layout_below="@id/tl"
        android:layout_marginTop="@dimen/dp_13"/>
    <ImageView
        android:id="@+id/autonym"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/shimingnan"
        android:layout_toRightOf="@id/tv_names"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_below="@id/tl"/>
    <TextView
        android:id="@+id/xain"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@color/grey"
        android:layout_below="@id/tv_names"
        android:layout_marginTop="@dimen/dp_17"
        android:layout_marginLeft="@dimen/dp_15"/>

    <TextView
        android:id="@+id/tv_identity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="身份状态公开"

        android:textSize="16sp"
        android:layout_marginTop="@dimen/dp_16"
        android:layout_marginLeft="@dimen/dp_12"
        android:layout_below="@id/xain"/>
    <Switch
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/dp_15"
        android:layout_marginLeft="@dimen/dp_203"
        android:layout_below="@id/xain"
        android:layout_marginTop="10dp"/>
    <TextView
        android:id="@+id/xian"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_10"
        android:layout_marginTop="@dimen/dp_17"
        android:layout_below="@id/tv_identity"
        android:background="@color/grey"/>
    <TextView
        android:id="@+id/modification"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/xian"
        android:layout_marginRight="@dimen/dp_40"
        android:layout_marginTop="@dimen/dp_18"
        android:layout_marginLeft="@dimen/dp_16"
        android:textSize="16sp"
        android:text="修改密码"
        />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/jinru"
        android:layout_alignParentRight="true"
        android:layout_below="@id/xian"
        android:layout_marginTop="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15"/>
    <TextView
        android:id="@+id/xian2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@color/grey"
        android:layout_below="@+id/modification"
        android:layout_marginTop="@dimen/dp_17"
        android:layout_marginLeft="@dimen/dp_15"
        />
    <TextView
        android:id="@+id/blacklist"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/xian2"
        android:layout_marginRight="@dimen/dp_40"
        android:layout_marginTop="@dimen/dp_18"
        android:layout_marginLeft="@dimen/dp_16"
        android:textSize="16sp"
        android:text="黑名单"
        />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/jinru"
        android:layout_alignParentRight="true"
        android:layout_below="@id/xian2"
        android:layout_marginTop="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15"/>
    <TextView
        android:id="@+id/xian3"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@color/grey"
        android:layout_below="@+id/blacklist"
        android:layout_marginTop="@dimen/dp_17"
        android:layout_marginLeft="@dimen/dp_15"
        />
    <TextView
        android:id="@+id/WeChat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/xian3"
        android:layout_marginRight="@dimen/dp_40"
        android:layout_marginTop="@dimen/dp_18"
        android:layout_marginLeft="@dimen/dp_16"
        android:textSize="16sp"
        android:text="微信"
        />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/jinru"
        android:layout_alignParentRight="true"
        android:layout_below="@id/xian3"
        android:layout_marginTop="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15"/>
    <TextView
        android:id="@+id/xian4"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_10"
        android:background="@color/grey"
        android:layout_below="@+id/WeChat"
        android:layout_marginTop="@dimen/dp_17"
        />
    <TextView
        android:id="@+id/hu_bo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="接收呼播信息"
        android:layout_below="@id/xian4"
        android:layout_marginLeft="@dimen/dp_16"
        android:layout_marginTop="@dimen/dp_18"/>
    <Switch
        android:id="@+id/scs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/hu_bo"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/dp_15"
        android:layout_below="@id/xian4"
        />
    <TextView
        android:id="@+id/xian5"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_10"
        android:background="@color/grey"
        android:layout_below="@+id/hu_bo"
        android:layout_marginTop="@dimen/dp_17"
        />
    <TextView
        android:id="@+id/guanyu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="关于"
        android:layout_marginRight="@dimen/dp_40"
        android:layout_below="@id/xian5"
        android:layout_marginLeft="@dimen/dp_16"
        android:layout_marginTop="@dimen/dp_18"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginTop="@dimen/dp_10"
        android:layout_alignParentRight="true"
        android:src="@mipmap/jinru"
        android:layout_marginRight="@dimen/dp_15"
        android:layout_below="@id/xian5"
        />
    <TextView
        android:id="@+id/xian6"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_10"
        android:background="@color/grey"
        android:layout_below="@+id/guanyu"
        android:layout_marginTop="@dimen/dp_17"
        />
    <TextView
        android:id="@+id/yijian"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="意见反馈"
        android:layout_below="@id/xian6"
        android:layout_marginRight="@dimen/dp_40"
        android:layout_marginLeft="@dimen/dp_16"
        android:layout_marginTop="@dimen/dp_18"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_15"
        android:src="@mipmap/jinru"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/dp_15"
        android:layout_below="@id/xian6"
        />
    <TextView
        android:id="@+id/xian7"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_10"
        android:background="@color/grey"
        android:layout_below="@+id/yijian"
        android:layout_marginTop="@dimen/dp_17"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="退出登录"
        android:textSize="16sp"
        android:layout_below="@id/xian7"
        android:textColor="@color/shop"
        android:layout_marginTop="@dimen/dp_18"
        android:layout_marginLeft="@dimen/dp_16"
        />

</RelativeLayout>