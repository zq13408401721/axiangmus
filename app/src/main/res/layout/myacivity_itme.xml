        <TextView
            android:id="@+id/tv_titl1"
            android:layout_width="@dimen/dp_194"
            android:layout_height="@dimen/dp_19"
            android:layout_below="@+id/tv_titl"
            android:layout_marginTop="@dimen/dp_6"
            android:text="@string/title1" />

        <TextView
            android:id="@+id/tv_titl2"
            android:layout_width="@dimen/dp_164"
            android:layout_height="@dimen/dp_19"
            android:layout_below="@+id/tv_titl1"
            android:layout_marginTop="@dimen/dp_6"
            android:text="截止时间：2019年8月31日" />
        <ImageView
            android:id="@+id/tou"
            android:layout_width="@dimen/dp_22"
            android:layout_height="@dimen/dp_22"
            android:src="@mipmap/tankuang"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="@dimen/dp_10"
            android:layout_marginRight="@dimen/dp_79"
            />
        <ImageView
            android:id="@+id/tou1"
            android:layout_width="@dimen/dp_22"
            android:layout_height="@dimen/dp_22"
            android:src="@mipmap/tankuang"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="@dimen/dp_10"
            android:layout_marginRight="@dimen/dp_68"
            />
        <ImageView
            android:id="@+id/tou2"
            android:layout_width="@dimen/dp_22"
            android:layout_height="@dimen/dp_22"
            android:src="@mipmap/tankuang"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="@dimen/dp_10"
            android:layout_marginRight="@dimen/dp_57"
            />
        <TextView
            android:id="@+id/renshu"
            android:layout_width="@dimen/dp_42"
            android:layout_height="@dimen/dp_10"
            android:textSize="@dimen/dp_10"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="@dimen/dp_10"
            android:layout_marginRight="@dimen/dp_10"
            />

    </RelativeLayout>
</RelativeLayout>
